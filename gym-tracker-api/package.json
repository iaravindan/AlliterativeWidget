{
  "name": "gym-tracker-api",
  "version": "1.0.0",
  "description": "Gym tracking API for Geofency webhooks with D1 database",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply gym-tracker-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply gym-tracker-db --remote",
    "db:create": "wrangler d1 create gym-tracker-db"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.24.0"
  }
}
